{
  "name": "golb-web-ui",
  "version": "0.0.4",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev-server": "BABEL_ENV=node NODE_ENV=development babel-node scripts/run-server.js",
    "webpack-dev-admin-client": "NODE_ENV=development webpack --config webpack.admin-client.config.js -w",
    "webpack-dev-public-client": "NODE_ENV=development webpack --config webpack.public-client.config.js -w",
    "compile": "npm-run-all compile:*",
    "compile:clear": "rimraf dist",
    "compile:server-apps": "BABEL_ENV=node NODE_ENV=production babel ./src --out-dir dist --copy-files",
    "compile:admin-client": "NODE_ENV=production webpack --config webpack.admin-client.config.js",
    "compile:public-client": "NODE_ENV=production webpack --config webpack.public-client.config.js",
    "prepublish": "npm run compile"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.0.2",
    "precss": "^1.4.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "draft-js": "^0.10.5",
    "draft-js-prism": "^1.0.6",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "node-fetch": "^1.7.3",
    "paginator": "^1.0.0",
    "prismjs": "^1.11.0",
    "prop-types": "^15.6.0",
    "query-string": "^4.3.4",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  }
}
