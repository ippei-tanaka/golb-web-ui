{
  "name": "golb-web-ui",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev-server": "GOLB_CONFIG=golb.config.dev.json BABEL_ENV=node NODE_ENV=development babel-node scripts/run-server.js -c golb.config.dev.json",
    "webpack-admin-dev": "GOLB_CONFIG=golb.config.dev.json NODE_ENV=development webpack --config webpack.admin-dev.config.js -w",
    "webpack-public-dev": "GOLB_CONFIG=golb.config.dev.json NODE_ENV=development webpack --config webpack.public-dev.config.js -w",
    "test": "mocha $(find test -name '*.test.js') --compilers js:babel-core/register",
    "compile": "npm-run-all compile:*",
    "compile:clear": "rimraf dist",
    "compile:build": "babel ./src --out-dir dist",
    "prepublish": "NODE_ENV=production npm run compile"
  },
  "devDependencies": {
    "argparse": "^1.0.9",
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-require-ignore": "^0.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "precss": "^1.4.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "express": "^4.15.2",
    "history": "^4.6.1",
    "html-webpack-plugin": "^2.28.0",
    "node-fetch": "^1.7.0",
    "npm-run-all": "^4.0.2",
    "paginator": "^1.0.0",
    "prop-types": "^15.5.8",
    "query-string": "^4.3.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  }
}
