{
  "name": "golb-web-ui",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel webpack-dev run-dev-server",
    "run-dev-server": "babel-node scripts/run-server.js -c config.dev.json",
    "webpack-dev": "GOLB_CONFIG=config.dev.json webpack --config webpack.dev.config.js -w",
    "test": "mocha $(find test -name '*.test.js') --compilers js:babel-core/register",
    "compile": "NODE_ENV=production npm-run-all compile:*",
    "compile:clear": "rimraf dist",
    "compile:build": "babel ./src --out-dir dist --copy-files --ignore bundle",
    "prepublish": "NODE_ENV=production npm run compile"
  },
  "devDependencies": {
    "argparse": "^1.0.9",
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "rimraf": "^2.6.1",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.28.0",
    "npm-run-all": "^4.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
